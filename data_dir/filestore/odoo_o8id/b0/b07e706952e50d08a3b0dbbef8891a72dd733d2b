            </t>
            <t t-if="!ui.isSmall and threadActions.actions.length &gt; 2">
                <t t-call="mail.ChatWindow.dropdownAction">
                    <t t-set="action" t-value="threadActions.actions.at(-2)"/>
                </t>
            </t>
