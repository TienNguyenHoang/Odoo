/**********************************************************************************************
*  Filepath: /mail/static/src/discuss/voice_message/common/attachment_uploader_hook_patch.js  *
*  Lines: 28                                                                                  *
**********************************************************************************************/
odoo.define('@mail/discuss/voice_message/common/attachment_uploader_hook_patch', ['@mail/core/common/attachment_upload_service', '@web/core/utils/patch'], function (require) {
'use strict';
let __exports = {};
/* @odoo-module */

const { AttachmentUploadService } = require("@mail/core/common/attachment_upload_service");

const { patch } = require("@web/core/utils/patch");

patch(AttachmentUploadService.prototype, {
    _makeAttachmentData(upload, tmpId, originThread, tmpUrl) {
        const attachmentData = super._makeAttachmentData(...arguments);
        if (upload.data.get("voice")) {
            attachmentData.voice = upload.data.get("voice");
        }
        return attachmentData;
    },
    _makeFormData(formData, file, hooker, tmpId, options) {
        super._makeFormData(...arguments);
        if (options?.isVoice) {
            formData.append("voice", options.isVoice);
        }
        return formData;
    },
});

return __exports;
});
;

/************************************************************************************
*  Filepath: /mail/static/src/discuss/voice_message/common/composer_model_patch.js  *
*  Lines: 17                                                                        *
************************************************************************************/
odoo.define('@mail/discuss/voice_message/common/composer_model_patch', ['@mail/core/common/composer_model', '@web/core/utils/patch'], function (require) {
