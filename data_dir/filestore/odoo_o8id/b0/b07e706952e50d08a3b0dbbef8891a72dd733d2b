odoo.define('@mail/discuss/gif_picker/common/picker_content_patch', ['@odoo/owl', '@web/core/utils/patch', '@web/core/utils/hooks', '@mail/discuss/gif_picker/common/gif_picker', '@mail/core/common/picker_content'], function (require) {
'use strict';
let __exports = {};
/* @odoo-module */

const { useState } = require("@odoo/owl");
const { patch } = require("@web/core/utils/patch");
const { useService } = require("@web/core/utils/hooks");
const { GifPicker } = require("@mail/discuss/gif_picker/common/gif_picker");
const { PickerContent } = require("@mail/core/common/picker_content");

Object.assign(PickerContent.components, { GifPicker });

patch(PickerContent.prototype, {
    setup() {
        super.setup();
        this.store = useState(useService("mail.store"));
        this.gifPickerService = useState(useService("discuss.gifPicker"));
    },
});

