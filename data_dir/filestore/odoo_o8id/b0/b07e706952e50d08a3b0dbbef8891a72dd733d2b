odoo.define('@mail/discuss/core/public_web/thread_service_patch', ['@mail/core/common/thread_service', '@web/core/utils/patch'], function (require) {
'use strict';
let __exports = {};
/* @odoo-module */

const { ThreadService } = require("@mail/core/common/thread_service");
const { patch } = require("@web/core/utils/patch");

patch(ThreadService.prototype, {
    /**
     * @override
     */
    setDiscussThread(thread) {
        super.setDiscussThread(...arguments);
        if (!thread.displayToSelf && thread.model === "discuss.channel") {
            thread.isLocallyPinned = true;
        }
    },
});

return __exports;
});
