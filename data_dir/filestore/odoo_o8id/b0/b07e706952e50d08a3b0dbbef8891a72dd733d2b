odoo.define('@mail/discuss/voice_message/common/attachment_service_patch', ['@mail/core/common/attachment_service', '@web/core/utils/patch'], function (require) {
'use strict';
let __exports = {};
/* @odoo-module */

const { AttachmentService, attachmentService } = require("@mail/core/common/attachment_service");
const { patch } = require("@web/core/utils/patch");

patch(AttachmentService.prototype, {
    setup(env, services) {
        super.setup(...arguments);
        /** @type {import("@mail/discuss/voice_message/common/voice_message_service").VoiceMessageService} */
        this.voiceMessageService = services["discuss.voice_message"];
    },
    remove(attachment) {
